@model LogoutViewModel


    <div class="card card-signin mx-auto" style="width: 24rem!important">
        <div class="card-body">
            <h2 class="card-title text-center">Logout</h2>
            <div class="text-center">
                <p>Would you like to logout of IdentityServer?</p>
                <form asp-action="Logout">
                    <input type="hidden" name="logoutId" value="@Model.LogoutId" />
                    <fieldset>
                        <div class="form-group">
                            <button class="btn btn-primary">Yes</button>
                        </div>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
 