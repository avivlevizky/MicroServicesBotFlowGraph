{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{controller}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "botflowgraph-webapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/{controller}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "IdentityApiKey",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/.well-known/openid-configuration",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "botflowgraph-identity",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/identityService/discovery"
    }
  ],
  "GlobalConfiguration": {
   
  }
}